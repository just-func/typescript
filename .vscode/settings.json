{
  "cSpell.words": [
    "macos",
    "pnpm",
    "snyk",
    "testname",
    "tsbuildinfo",
    "turborepo"
  ],
  "files.associations": {
    ".depcheckrc": "yaml"
  },
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "eslint.nodePath": ".yarn/sdks",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true
}
